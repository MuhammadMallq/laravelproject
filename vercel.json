{
    "version": 2,
    "framework": null,
    "buildCommand": "",
    "outputDirectory": ".",
    "functions": {
        "api/*.php": {
            "runtime": "vercel-php@0.9.0"
        }
    },
    "routes": [
        {
            "src": "/build/(.*)",
            "dest": "/public/build/$1"
        },
        {
            "src": "/img/(.*)",
            "dest": "/public/img/$1"
        },
        {
            "src": "/logo.png",
            "dest": "/public/logo.png"
        },
        {
            "src": "/image.png",
            "dest": "/public/image.png"
        },
        {
            "src": "/favicon.ico",
            "dest": "/public/favicon.ico"
        },
        {
            "src": "/(.*)",
            "dest": "/api/index.php"
        }
    ]
}