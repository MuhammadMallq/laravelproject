{
    "version": 2,
    "framework": null,
    "installCommand": "composer install --no-dev --optimize-autoloader",
    "buildCommand": "",
    "outputDirectory": "",
    "builds": [
        {
            "src": "api/index.php",
            "use": "vercel-community/php@0.9.0"
        },
        {
            "src": "public/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/build/(.*)",
            "dest": "/public/build/$1"
        },
        {
            "src": "/img/(.*)",
            "dest": "/public/img/$1"
        },
        {
            "src": "/logo.png",
            "dest": "/public/logo.png"
        },
        {
            "src": "/image.png",
            "dest": "/public/image.png"
        },
        {
            "src": "/favicon.ico",
            "dest": "/public/favicon.ico"
        },
        {
            "src": "/(.*)",
            "dest": "/api/index.php"
        }
    ]
}